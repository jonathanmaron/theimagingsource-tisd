{
    "name": "theimagingsource/tisd",
    "description": "PHP wrapper for TIS Download System Web API. Authored and supported by The Imaging Source Europe GmbH."
    "homepage": "https://github.com/jonathanmaron/theimagingsource-tisd",
    "type": "library",
    "license": "Apache-2.0",
    "config": {
        "optimize-autoloader": true,
        "classmap-authoritative": true
    },
    "authors": [
        {
            "name": "Jonathan Maron (The Imaging Source)",
            "homepage": "https://github.com/jonathanmaron/",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Tisd\\Sdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TisdTest\\Sdk\\": "test/"
        }
    },
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4",
        "symfony/var-dumper": "^4.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
